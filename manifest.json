{

  "manifest_version": 2,
  "name": "fb-secure-msg",
  "version": "1.0",

  "permissions": [
      "https://www.facebook.com/*"
    ],

  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/messages/*"],
      "js": ["fb-secure-msg.js"]
    }
  ],

  "background": {
    "scripts":["background.js"]
  }
}